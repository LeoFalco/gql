{
  "name": "gql",
  "version": "1.0.125",
  "main": "src/index.ts",
  "license": "MIT",
  "private": true,
  "repository": "https://github.com/LeoFalco/gql",
  "workspaces": [
    "./"
  ],
  "scripts": {
    "clean": "npx lerna clean -y",
    "start-db": "npm run create-database",
    "pretest": "npm run create-database",
    "create-database": "npx dotenv -e .env.$(echo $ENVIRONMENT) -- npx ts-node bin/create-database.ts",
    "migrate-dev": "npx dotenv -e .env.$(echo $ENVIRONMENT) -- npx prisma migrate dev --preview-feature",
    "migrate": "npx dotenv -e .env.$(echo $ENVIRONMENT) -- npx prisma migrate deploy --preview-feature",
    "test": "npm run migrate && npm run jest",
    "jest": "npx dotenv -e .env.$(echo $ENVIRONMENT) -- npx jest --detectOpenHandles --forceExit",
    "start": "npm run migrate && npm run start-ts-node",
    "start-ts-node": "npx dotenv -e .env.$(echo $ENVIRONMENT) -- npx ts-node .",
    "postinstall": "npx dotenv -e .env.$(echo $ENVIRONMENT) -- npx prisma generate",
    "seed": "npx dotenv -e .env.$(echo $ENVIRONMENT) -- npx ts-node bin/seed",
    "nodemon": "npx dotenv -e .env.$(echo $ENVIRONMENT) -- npx nodemon",
    "start-dev": "npm run nodemon",
    "lint": "npx eslint .",
    "lint-fix": "npx eslint . --fix",
    "commit": "npx cz"
  },
  "dependencies": {
    "@prisma/cli": "2.17.0",
    "@prisma/client": "2.17.0",
    "@sentry/node": "6.2.1",
    "@sentry/tracing": "6.2.1",
    "apollo-boost": "0.4.9",
    "apollo-server": "2.21.0",
    "async": "3.2.0",
    "commitizen": "4.2.3",
    "cuid": "2.1.8",
    "dotenv": "8.2.0",
    "dotenv-cli": "4.0.0",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-prettier": "3.3.1",
    "graphql": "15.5.0",
    "npm": "7.6.0",
    "pg": "8.5.1",
    "postinstall": "0.7.0",
    "sql-template-strings": "2.2.2",
    "ts-node": "9.1.1",
    "typescript": "4.2.2"
  },
  "devDependencies": {
    "@commitlint/cli": "12.0.1",
    "@commitlint/config-conventional": "12.0.1",
    "@sentry/cli": "1.63.0",
    "@types/cross-spawn": "6.0.2",
    "@types/jest": "26.0.20",
    "@types/node": "10.17.54",
    "@types/node-fetch": "2.5.8",
    "@types/pg": "7.14.10",
    "@typescript-eslint/eslint-plugin": "4.16.1",
    "@typescript-eslint/parser": "4.16.1",
    "apollo-server-testing": "2.21.0",
    "chalk": "4.1.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.21.0",
    "eslint-config-standard": "16.0.2",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.1.5",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.3.1",
    "eslint-plugin-standard": "5.0.0",
    "husky": "5.1.3",
    "jest": "26.6.3",
    "lerna": "4.0.0",
    "nodemon": "2.0.7",
    "prettier": "2.2.1",
    "rimraf": "3.0.2",
    "sucrase": "3.17.1",
    "supertest": "6.1.3",
    "ts-jest": "26.5.2",
    "tslint": "6.1.3"
  },
  "engineStrict": true,
  "engines": {
    "node": "10.24.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
